	      value: ${s:objColValue("实体对象名","编码字段","名称字段", "编码值", "缺省返回值")}
	    </example>  
 	</function>

    <function>
 		<description>获取资源列表（json格式）</description>
	    <name>resJsonList</name>
	    <function-class>com.clj.reptilehouse.common.util.WebFuncUtil</function-class>
	    <function-signature>String getResJsonList(java.lang.String, java.lang.String, java.lang.String)</function-signature>
	    <example>
	      value: ${s:resJsonList("资源组编码","过滤条件","json属性名称（资源编码,资源名称）")}
	    </example>  
 	</function>    	

    <function>
 		<description>获取资源树（json格式）</description>
	    <name>resJsonTree</name>
	    <function-class>com.clj.reptilehouse.common.util.WebFuncUtil</function-class>
	    <function-signature>String getResJsonTree(java.lang.String, java.lang.String, java.lang.String, java.lang.String)</function-signature>
