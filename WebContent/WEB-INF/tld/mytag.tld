    <function>
 		<description>根据资源编码获取资源内容</description>
	    <name>resVal</name>
	    <function-class>com.clj.reptilehouse.common.util.WebFuncUtil</function-class>
	    <function-signature>String getResValue(java.lang.String, java.lang.String)</function-signature>
	    <example>
	      value: ${s:resVal("资源组编码","资源编码")}
	    </example>  
 	</function>    
 		
    <function>
 		<description>根据资源编码获取资源内容</description>
	    <name>resValue</name>
	    <function-class>com.clj.reptilehouse.common.util.WebFuncUtil</function-class>
	    <function-signature>String getResValue(java.lang.String, java.lang.String, java.lang.String)</function-signature>
	    <example>
	      value: ${s:resValue("资源组编码","资源编码","缺省返回值")}
	    </example>  
 	</function>    	
    
    <function>
 		<description>根据参数编码获取参数值</description>
	    <name>paramVal</name>
	    <function-class>com.clj.reptilehouse.common.util.WebFuncUtil</function-class>
	    <function-signature>String getParamValue(java.lang.String)</function-signature>
	    <example>
	      value: ${s:paramVal("参数编码")}
	    </example>  
 	</function>   
 	 	
    <function>
 		<description>根据参数编码获取参数值</description>
