	    <function-signature>String getParamValue(java.lang.String, java.lang.String)</function-signature>
	    <example>
	      value: ${s:paramValue("参数编码", "缺省返回值")}
	    </example>  
 	</function>    	
 	
     <function>
 		<description>根据指定对象的指定字段值，获取另一字段值</description>
	    <name>objColVal</name>
	    <function-class>com.clj.reptilehouse.common.util.WebFuncUtil</function-class>
	    <function-signature>String getObjColValue(java.lang.String, java.lang.String, java.lang.String, java.lang.String)</function-signature>
	    <example>
	      value: ${s:objColVal("实体对象名","编码字段","名称字段", "编码字段值")}
	    </example>  
 	</function>
 	
